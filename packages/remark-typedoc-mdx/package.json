{
  "name": "remark-typedoc-mdx",
  "author": "Mayank Kumar Chaudhari <https://mayankchaudhari.com>",
  "private": false,
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react18-tools/turbo-forge",
    "directory": "packages/remark-typedoc-mdx"
  },
  "bugs": "https://github.com/react18-tools/turbo-forge/issues",
  "homepage": "https://github.com/react18-tools/turbo-forge/blob/main/packages/remark-typedoc-mdx/README.md",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup && gzip -c dist/index.js | wc -c",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/node": "latest",
    "tsup": "latest",
    "typescript": "latest",
    "unified": "^11.0.5"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/react18-tools"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/mayank1513"
    }
  ],
  "keywords": [
    "remark-typedoc-mdx",
    "turbo-forge"
  ],
  "dependencies": {
    "unist-util-visit": "^5.1.0"
  }
}

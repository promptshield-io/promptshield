{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Turbo-Forge Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "incremental": true,
    "resolveJsonModule": true
  }
}
